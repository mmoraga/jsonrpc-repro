<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net6.0-ios</TargetFramework>
    <RuntimeIdentifier>ios-arm64</RuntimeIdentifier>
    <RootNamespace>jsonrpc_repro</RootNamespace>
    <ApplicationId>com.foo.app</ApplicationId>
    <CodesignKey>Apple Development</CodesignKey>
    <OutputType>Exe</OutputType>
    <Nullable>enable</Nullable>
    <ImplicitUsings>true</ImplicitUsings>
    <SupportedOSPlatformVersion>15.2</SupportedOSPlatformVersion>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="StreamJsonRpc" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="StreamJsonRpc" Version="2.12.27" />
  </ItemGroup>
</Project>
